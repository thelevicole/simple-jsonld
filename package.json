  "private": true,
  "dependencies": {
    "codemirror": "^5.59.2",
    "jsonlint-mod": "^1.7.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jquery-edittable": "^0.2.2",
    "laravel-mix": "^5.0.4",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
