  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "ie 8",
    "ie 7"
  ],
  "scripts": {
    "local": "cross-env NODE_ENV=local node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
